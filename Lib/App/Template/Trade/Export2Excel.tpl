 <Worksheet ss:Name="Sheet1">
  <Table x:fullcolumns="1"
   x:FullRows="1" ss:defaultcolumnwidth="54" ss:defaultrowheight="14.25">
   <Column ss:AutoFitWidth="0" ss:Width="68.25"/>
   <Column ss:AutoFitWidth="0" ss:Width="82.5"/>
   <Column ss:AutoFitWidth="0" ss:Width="59.25"/>
   <Column ss:AutoFitWidth="0" ss:Width="68.25"/>
   {foreach from=$arr_field_value.Products item=item} 
{section name=row loop=$item.boxCnt}
   <Row ss:AutoFitHeight="0" ss:Height="24">
    <Cell ss:MergeAcross="3" ss:StyleID="m23741028"><Data ss:Type="String">多 顺 电 器 装 箱 单</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="24">
    <Cell ss:StyleID="s28"><Data ss:Type="String">购 货 方</Data></Cell>
    <Cell ss:MergeAcross="2" ss:StyleID="m23741038"><Data ss:Type="String">{$arr_field_value.Client.compName}</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="24">
    <Cell ss:StyleID="s28"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40">名称<Font html:face="Times New Roman"
       x:Family="Roman">/</Font><Font>型号</Font></ss:Data></Cell>
    <Cell ss:MergeAcross="2" ss:StyleID="m23741048"><Data ss:Type="String">{$item.proName|default:'&nbsp;'}</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="24">
    <Cell ss:StyleID="s28"><Data ss:Type="String">订单总量</Data></Cell>
    <Cell ss:StyleID="s29"><Data ss:Type="String">{$item.cnt|default:'&nbsp;'}</Data></Cell>
    <Cell ss:StyleID="s28"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40">总<Font html:face="Times New Roman"
       x:Family="Roman"> </Font><Font>箱</Font><Font html:face="Times New Roman"
       x:Family="Roman"> </Font><Font>数</Font></ss:Data></Cell>
    <Cell ss:StyleID="s29"><Data ss:Type="String">{$item.boxCnt|default:'&nbsp;'}</Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="24">
    <Cell ss:StyleID="s28"><Data ss:Type="String">每箱数量</Data></Cell>
    <Cell ss:StyleID="s29"><Data ss:Type="String">{$item.boxCnt|default:'&nbsp;'}</Data></Cell>
    <Cell ss:StyleID="s28"><Data ss:Type="String">线长</Data></Cell>
    <Cell ss:StyleID="s29"/>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="24">
    <Cell ss:StyleID="s28"><Data ss:Type="String">箱编码</Data></Cell>
    <Cell ss:StyleID="s36"/>
    <Cell ss:StyleID="s28"><Data ss:Type="String">插件型号</Data></Cell>
    <Cell ss:StyleID="s29"/>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="24">
    <Cell ss:StyleID="s28"><Data ss:Type="String">输出电压</Data></Cell>
    <Cell ss:StyleID="s36"/>
    <Cell ss:StyleID="s28"><Data ss:Type="String">插件排列</Data></Cell>
    <Cell ss:StyleID="s29"/>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="24">
    <Cell ss:StyleID="s28"><Data ss:Type="String">生产日期</Data></Cell>
    <Cell ss:StyleID="s37"><Data ss:Type="String">{$arr_field_value.dateOrder}</Data></Cell>
    <Cell ss:StyleID="s28"><ss:Data ss:Type="String"
      xmlns="http://www.w3.org/TR/REC-html40">检<Font html:face="Times New Roman"
       x:Family="Roman"> </Font><Font>验</Font><Font html:face="Times New Roman"
       x:Family="Roman"> </Font><Font>员</Font></ss:Data></Cell>
